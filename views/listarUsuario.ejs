<%- include("./partials/header") %>

<div class="container">
  <div class="card" id="cardListarUsuario">
      <div class="row">
        <div class="col m6">
            <h5>Listagem dos Usuarios</h5>
        </div>
        <div class="col m6"> 
            <a class="blue btn right" href="/login/cadastrarUsuario">Cadastrar Novo Usuario</a>       
        </div>
      </div>
        <table>                  
            <thead>
            <tr>
                <th>Nome</th>
                <th>Email</th>
                <th>Acoes</th>
            </tr>
            </thead>

            <tbody>
            <% for(let i=0; i < usuarios.length; i++){ %>
            <tr>        
                <td><%= usuarios[i].nome %></td>
                <td><%= usuarios[i].email %></td>
                <td>
                    <a href="">Editar</a>
                    <a href="">Excluir</a>
                    <a href="">Visualizar</a>
                </td>       
            </tr>    
            <% } %>    
            </tbody>
        </table>
    </div>
</div>
        
<%- include("./partials/footer") %>